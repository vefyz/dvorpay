{
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "echo 'Building...'",
  "outputDirectory": ".",
  "functions": {
    "api/index.py": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
